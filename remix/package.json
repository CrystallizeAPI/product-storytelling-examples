{
  "private": true,
  "name": "remix-app-template",
  "description": "",
  "license": "",
  "scripts": {
    "build": "npm run build:css && remix build",
    "build:css": "tailwindcss -i ./app/styles/index.css -o ./app/styles/tailwind.css --minify",
    "dev": "concurrently \"npm run dev:css\" \"remix dev\"",
    "dev:css": "tailwindcss -i ./app/styles/index.css -o ./app/styles/tailwind.css --watch",
    "postinstall": "remix setup node",
    "start": "remix-serve build",
    "generate": "yarn generate:pim && yarn generate:service-api",
    "generate:pim": "graphql-codegen --config codegen.pim.yml",
    "generate:service-api": "graphql-codegen --config codegen.service-api.yml"
  },
  "dependencies": {
    "@crystallize/grid-renderer": "^2.1.1",
    "@crystallize/react-content-transformer": "^2.0.0",
    "@crystallize/react-image": "^7.3.1",
    "@graphql-codegen/typescript": "^2.4.2",
    "@remix-run/react": "^1.1.3",
    "@remix-run/serve": "^1.1.3",
    "@stitches/react": "^1.2.6",
    "@stripe/react-stripe-js": "^1.7.0",
    "@stripe/stripe-js": "^1.22.0",
    "@tailwindcss/forms": "^0.4.0",
    "axios": "^0.25.0",
    "dotenv": "^16.0.0",
    "graphql": "^16.3.0",
    "graphql-request": "^4.0.0",
    "immer": "^9.0.12",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-player": "^2.9.0",
    "react-query": "^3.34.12",
    "remix": "^1.1.3",
    "@netlify/functions": "^0.10.0",
    "@remix-run/netlify": "^1.1.3"
  },
  "devDependencies": {
    "@babel/core": "^7.15.0",
    "@graphql-codegen/cli": "^2",
    "@graphql-codegen/fragment-matcher": "^2",
    "@graphql-codegen/gql-tag-operations-preset": "^1.0.1",
    "@graphql-codegen/near-operation-file-preset": "^2",
    "@graphql-codegen/typed-document-node": "^2",
    "@graphql-codegen/typescript": "^2",
    "@graphql-codegen/typescript-operations": "^2",
    "@graphql-typed-document-node/core": "^2",
    "@remix-run/dev": "^1.1.3",
    "@storybook/addon-actions": "^6.3.7",
    "@storybook/addon-essentials": "^6.3.7",
    "@storybook/addon-links": "^6.3.7",
    "@storybook/react": "^6.3.7",
    "@types/lodash": "^4.14.172",
    "@types/node": "^16.4.12",
    "@types/react": "^17.0.24",
    "@types/react-dom": "^17.0.9",
    "autoprefixer": "^10.4.2",
    "babel-loader": "^8.2.2",
    "concurrently": "^7.0.0",
    "graphql": "^15.5.1",
    "postcss": "^8.4.6",
    "prettier": "^2.3.2",
    "tailwindcss": "^3.0.18",
    "typescript": "^4.1.2",
    "vercel": "^23.1.2"
  },
  "engines": {
    "node": ">=14"
  },
  "sideEffects": false
}
